{
  "name": "nexora-backend-api",
  "version": "1.2.0",
  "description": "Nexora AED Platform - Backend API Services",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "docker:build": "docker build -t nexora-api .",
    "docker:run": "docker run -p 8080:8080 nexora-api",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f nexora-api",
    "security:audit": "npm audit",
    "security:scan": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "docs:generate": "swagger-jsdoc -d src/docs/swagger.ts src/routes/*.ts -o docs/swagger.json",
    "docs:serve": "swagger-ui-serve docs/swagger.json",
    "metrics:test": "curl http://localhost:8080/metrics",
    "health:check": "curl http://localhost:8080/health"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.943.0",
    "@aws-sdk/client-iam": "^3.943.0",
    "@azure/identity": "^4.0.1",
    "@google-cloud/secret-manager": "^5.0.1",
    "@prisma/client": "^5.7.1",
    "@types/pg": "^8.15.6",
    "async-mutex": "^0.5.0",
    "aws-sdk": "^2.1506.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "ipaddr.js": "^2.3.0",
    "isomorphic-dompurify": "^2.33.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-vault": "^0.10.0",
    "nodemailer": "^6.9.7",
    "opossum": "^9.0.0",
    "pg": "^8.16.3",
    "prisma": "^5.7.1",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^4.6.11",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-loki": "^6.0.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/opossum": "^8.1.9",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
